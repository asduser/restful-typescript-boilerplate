{
  "rules": {
    "max-line-length": [true, 120],
    "class-name": true,
    "indent": [true, "spaces"],
    "no-duplicate-variable": true,
    "no-eval": true,
    "one-line": [true, "check-open-brace"],
    "semicolon": false,
    "variable-name": [true, "ban-keywords"],
    "eofline": true
  }
}